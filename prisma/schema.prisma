// prisma/schema.prisma

model User {
  id       Int       @id @default(autoincrement())
  email    String    @unique
  contacts Contact[] // One user has many contacts
  password String
}

model Contact {
  id      Int    @id @default(autoincrement())
  email   String
  userId  Int
  user    User   @relation(fields: [userId], references: [id])
}
